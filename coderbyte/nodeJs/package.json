{
  "name": "sample",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean-modules": "rm node_modules -R;",
    "setup-modules": "npm install;",
    "build": "npm run clean-dist && npm run copy-files && tsc --watch --preserveWatchOutput",
    "clean-dist": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/*.js src/css/*.css dist/",
    "start": "concurrently \"npm run build\" \"nodemon dist/app/index.js --trace-uncaught\"",
    "test": "cross-env TS_NODE_FILES=true mocha --exit --require ts-node/register --colors src/**/*.spec.ts",
    "coverage": "nyc npm run test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "csv-parser": "^3.0.0",
    "express": "^4.17.1",
    "line-reader": "^0.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.17.11",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.1",
    "chai": "^4.2.0",
    "concurrently": "^6.0.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.2.0",
    "nodemon": "^2.0.7",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "typescript": "~4.1.5",
    "rimraf": "^3.0.2",
    "copyfiles": "^2.4.1"
  }
}
